<odoo>
    <data>
      <!--Access Top category Group-->

      <record model="ir.module.category" id="student_registration_category">
        <field name="name">Studen Registration Management</field>
      </record>

      <!--Access Groups-->

      <record id="group_student_registration_admin" model="res.groups">
        <field name="name">Administrator</field>
        <field name="category_id" ref="student_registration_category"/>
      </record>

      <record id="group_student_registration_manager" model="res.groups">
        <field name="name">Coordinator</field>
        <field name="category_id" ref="student_registration_category"/>
      </record>

      <record id="group_student_registration_teacher" model="res.groups">
        <field name="name">Teacher</field>
        <field name="category_id" ref="student_registration_category"/>
      </record>

      <record id="group_student_registration_student" model="res.groups">
        <field name="name">Student</field>
        <field name="category_id" ref="student_registration_category"/>
      </record>

    </data>
</odoo>